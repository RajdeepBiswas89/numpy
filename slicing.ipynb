   "metadata": {},
   "source": [
    "slicing in 1d matrix or array"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "db5451f7-2c45-4f21-8249-382379b805d1",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[1 2 4 5 6]\n",
      "[ 1  2  4  5  6  8  6  6  3  4 67  7]\n",
      "[ 1  4  6  6  3 67]\n"
     ]
    }
   ],
   "source": [
    "import numpy as np\n",
    "arr= np.array([1,2,4,5,6,8,6,6,3,4,67,7]) \n",
    "print(arr[0:5])\n",
    "print(arr[:])\n",
    "print(arr[::2]) #stepsize=2"
   ]
  },
